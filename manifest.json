{
    "manifest_version" : 2,
    "name" : "AutoDirectExtension2",
    "version" : "2.0",
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "background": {
        "persistent": false,
        "scripts": ["back.js"]
    },
    "permissions": [
        "storage",
        "tabs",
        "bookmarks"
    ]
}